#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./inventoryapidocker clean install -DskipTests"
          bat "docker build -f inventoryapidocker/dockerfile -t invapp ./inventoryapidocker"
                                   
       }
   
   }
 
 }
 
 
 
}
